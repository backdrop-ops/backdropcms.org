        "1bf26084-5e1f-49da-9e4a-74d812df73ba": {
            "plugin": "layout:custom_block",
            "data": {
                "status": 1,
                "module": "layout",
                "delta": "custom_block",
                "settings": {
                    "title_display": "default",
                    "title": "Backdrop Forum",
                    "style": "default",
                    "block_settings": [],
                    "contexts": [],
                    "content": "<p>Join&nbsp;the Backdrop Forum and&nbsp;exchange&nbsp;ideas with the rest of the community in the form of&nbsp;questions, answers, and discussions on any given topic.</p>\r\n\r\n<p><i class=\"fa fa-comments\"></i> <a href=\"https://forum.backdropcms.org\">Join our Forum</a></p>\r\n",
                    "format": "full_html"
                },
                "uuid": "1bf26084-5e1f-49da-9e4a-74d812df73ba",
                "style": {
                    "plugin": "default",
                    "data": {
                        "settings": {
