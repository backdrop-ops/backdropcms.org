                    "style": "default",
                    "block_settings": [],
                    "contexts": [],
                    "content": "<p>\r\n    <i class=\"fa fa-commenting\"></i> Drop in and say hi in <a href=\"https://backdrop.zulipchat.com\">Live chat</a> with Zulip.\r\n</p>",
                    "format": "full_html",
                    "admin_label": "",
                    "admin_description": ""
