        "fe4084b4-503a-4acf-8b47-71644f3dd63f": {
            "plugin": "layout:custom_block",
            "data": {
                "module": "layout",
                "delta": "custom_block",
                "settings": {
                    "title_display": "default",
                    "title": "",
                    "style": "default",
                    "block_settings": [],
                    "contexts": [],
                    "content": "<p>You can change your mind at any time by clicking the \"unsubscribe\" link, included&nbsp;in the footer of every&nbsp;newsletter email.&nbsp;</p>\r\n",
                    "format": "filtered_html"
                },
                "uuid": "fe4084b4-503a-4acf-8b47-71644f3dd63f",
