# Ignore this file so it can be changed
.gitignore

# Ignore the ssl .well-known
.well-known

# Ignore configuration files that may contain sensitive information.
settings.local.php
sites/*/settings.local.php
civicrm.settings.local.php
sites/*/civicrm.settings.local.php

# Ignore files that change for local installations
civicrm_table_prefixes.php
sites/*/civicrm_table_prefixes.php

# Ignore paths that contain user-generated content.
files
private
sites/*/files
sites/*/private
